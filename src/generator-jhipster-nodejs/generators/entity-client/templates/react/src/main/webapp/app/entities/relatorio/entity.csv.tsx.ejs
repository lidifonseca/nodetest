      <%_ } else { _%>
            <%_ if (dto === 'no') { _%>
      {<%= entityInstance + "." + relationshipFieldName %> ?
      <Link to={`<%= otherEntityStateName %>/${<%= entityInstance + "." + relationshipFieldName + ".id}" %>`}>
        {<%= entityInstance + "." + relationshipFieldName + "." + otherEntityField %>}
      </Link> : ''}
            <%_ } else { _%>
      {<%= entityInstance + "." + relationshipFieldName %> ?
      <Link to={`<%= otherEntityStateName %>/${<%= entityInstance + "." + relationshipFieldName + ".id}" %>`}>
        {<%= entityInstance + "." + relationshipFieldName + ".id" %>}
      </Link> : ''}
            <%_ } _%>
        <%_ } _%>
      <%_ } _%>
    </td>
    <%_ } _%>
<%_ } _%>

